n    return sum > 0;\n  };\n\n  const purchaseHandler = () => {\n    if (props.isAuthenticated) {\n      setPurchasing(true);\n    } else {\n      props.onSetAuthRedirectPath(\"/checkout\");\n      props.history.push(\"/auth\");\n    }\n  };\n\n  const purchaseCancelHandler = () => {\n    setPurchasing(false);\n  };\n\n  const purchaseContinueHandler = () => {\n    props.onInitPurchase();\n    props.history.push(\"/checkout\");\n  };\n\n  const disabledInfo = {\n    ...props.ings,\n  };\n  for (let key in disabledInfo) {\n    disabledInfo[key] = disabledInfo[key] <= 0;\n  }\n  let orderSummary = null;\n\n  let burger = <Spinner />;\n  if (props.ings) {\n    burger = (\n      <Aux>\n        <Burger ingredients={props.ings} />\n        <BuildControls\n          ingredientAdded={props.onIngredientAdded}\n          ingredientRemoved={props.onIngredientRemoved}\n          disabled={disabledInfo}\n          purchasable={!updatePurchaseState(props.ings)}\n          ordered={purchaseHandler}\n          isAuth={props.isAuthenticated}\n          price={props.price}\n        />\n      </Aux>\n    );\n    orderSummary = (\n      <OrderSummary\n        ingredients={props.ings}\n        purchaseCancelled={purchaseCancelHandler}\n        purchaseContinued={purchaseContinueHandler}\n        price={props.price}\n      />\n    );\n  }\n  return (\n    <Aux>\n      <Modal show={purchasing} modalClosed={purchaseCancelHandler}>\n        {orderSummary}\n      </Modal>\n      {burger}\n    </Aux>\n  );\n};\n\nconst mapStateToProps = (state) => {\n  return {\n    ings: state.burgerBuilder.ingredients,\n    price: state.burgerBuilder.totalPrice,\n    error: state.burgerBuilder.error,\n    isAuthenticated: state.auth.token !== null,\n  };\n};\n\nconst mapDispatchToProps = (dispatch) => {\n  return {\n    onIngredientAdded: (ingName) => dispatch(actions.addIngredient(ingName)),\n    onIngredientRemoved: (ingName) =>\n      dispatch(actions.removeIngredient(ingName)),\n    onInitIngredients: () => {\n      dispatch(actions.initIngredients());\n    },\n    onInitPurchase: () => dispatch(actions.purchaseInit()),\n    onSetAuthRedirectPath: (path) =>\n      dispatch(actions.setAuthRedirectPath(path)),\n  };\n};\n\nexport default connect(\n  mapStateToProps,\n  mapDispatchToProps\n)(withErrorHandler(BurgerBuilder, axios));\n","/Users/mateuszgadecki/Desktop/React/burger_builder/src/components/Burger/Burger.js",[],"/Users/mateuszgadecki/Desktop/React/burger_builder/src/components/Burger/BurgerIngredient/BurgerIngredient.js",[],"/Users/mateuszgadecki/Desktop/React/burger_builder/src/components/Burger/BuildControls/BuildControls.js",[],"/Users/mateuszgadecki/Desktop/React/burger_builder/src/components/Burger/BuildControls/BuildControl/BuildControl.js",[],"/Users/mateuszgadecki/Desktop/React/burger_builder/src/components/UI/Modal/Modal.js",[],"/Users/mateuszgadecki/Desktop/React/burger_builder/src/components/Burger/OrderSummary/OrderSummary.js",[],"/Users/mateuszgadecki/Desktop/React/burger_builder/src/components/UI/Backdrop/Backdrop.js",[],"/Users/mateuszgadecki/Desktop/React/burger_builder/src/components/UI/Button/Button.js",[],"/Users/mateuszgadecki/Desktop/React/burger_builder/src/components/Navigation/Toolbar/Toolbar.js",[],"/Users/mateuszgadecki/Desktop/React/burger_builder/src/components/Logo/Logo.js",[],"/Users/mateuszgadecki/Desktop/React/burger_builder/src/components/Navigation/NavigationItems/NavigationItems.js",[],"/Users/mateuszgadecki/Desktop/React/burger_builder/src/components/Navigation/NavigationItems/NavigationItem/NavigationItem.js",[],["175","176"],"/Users/mateuszgadecki/Desktop/React/burger_builder/src/components/Navigation/SideDrawer/SideDrawer.js",[],"/Users/mateuszgadecki/Desktop/React/burger_builder/src/components/Navigation/SideDrawer/DrawerToggle/DrawerToggle.js",[],"/Users/mateuszgadecki/Desktop/React/burger_builder/src/hoc/Aux/Aux.js",[],"/Users/mateuszgadecki/Desktop/React/burger_builder/src/hoc/Layout/Layout.js",[],"/Users/mateuszga